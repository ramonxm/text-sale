model TB_CENA_FIGURANTE {
  id                     BigInt       @id @default(autoincrement())
  id_cena                BigInt
  id_figurante           BigInt
  cd_usuario_criacao     String       @db.VarChar(50)
  cd_usuario_modificacao String       @db.VarChar(50)
