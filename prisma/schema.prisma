  TB_CENA_TB_CENA_SEQUENCIA_id_cena_relacionadaToTB_CENA TB_CENA  @relation("TB_CENA_SEQUENCIA_id_cena_relacionadaToTB_CENA", fields: [id_cena_relacionada], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "TB_DISPOSITIVO_TB_CENA_RELACIONADA")

  @@id([id_cena, id_cena_relacionada, id_ordem])
  @@index([id_cena_relacionada], map: "TB_DISPOSITIVO_TB_CENA_RELACIONADA")
