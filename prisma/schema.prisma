  id_cena                BigInt
  id_roteiro             Int
  id_tp_execucao         Int              @db.TinyInt
  dt_execucao            DateTime         @db.DateTime(0)
  st_hora_manual         Boolean          @default(dbgenerated("b'0'")) @db.Bit(1)
  st_ativo               Boolean          @default(dbgenerated("b'1'")) @db.Bit(1)
  cd_usuario_criacao     String           @db.VarChar(50)
  cd_usuario_modificacao String           @db.VarChar(50)
  dt_criacao             DateTime         @db.DateTime(0)
  dt_modificacao         DateTime         @db.DateTime(0)
  TB_TIPO_EXECUCAO       TB_TIPO_EXECUCAO @relation(fields: [id_tp_execucao], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "CENA_EXECUCAO_TIPO_EXECUCAO")

  @@index([id_tp_execucao], map: "CENA_EXECUCAO_TIPO_EXECUCAO_IDX")
  @@index([id_cena, st_ativo], map: "TB_CENA_EXECUCAO_id_cena_IDX")
