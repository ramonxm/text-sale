  TB_CENA                  TB_CENA        @relation(fields: [id_cena], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "TB_CENA_PERSONAGEM_TEMPORARIO_CENA")

  @@unique([id_personagem, id_cena, nm_temporario_personagem], map: "UNIQUE_PERSONAGEM_CENA_NOME")
  @@index([id_personagem], map: "TB_CENA_PERSONAGEM_PERSONAGEM_idx")
  @@index([id_cena], map: "TB_CENA_PERSONAGEM_TEMPORARIO_CENA")
  @@index([nm_temporario_personagem], map: "TB_CENA_PERSONAGEM_nm_temporario_personagem_index")
}

