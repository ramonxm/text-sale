  @@index([st_ativo, id_programa], map: "PROGRAMA_ATIVO_IX")
}

model TB_CENARIO_SINONIMO {
  id                     BigInt     @id @default(autoincrement())
  id_cenario             BigInt
  nm_cenario             String     @db.VarChar(50)
