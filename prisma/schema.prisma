  dt_modificacao         DateTime   @db.DateTime(0)
  TB_CENARIO             TB_CENARIO @relation(fields: [id_cenario], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "CENARIO_FK")

  @@index([id_cenario], map: "CENARIO_FK_IX")
}

model TB_CENA_EXECUCAO {
