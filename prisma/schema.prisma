generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TB_AMBIENTE {
  id                         BigInt     @id @default(autoincrement())
  id_cenario                 BigInt
  id_tp_classificacao_padrao Int?       @db.TinyInt
  nm_ambiente                String     @db.VarChar(50)
  cd_usuario_criacao         String     @db.VarChar(50)
  cd_usuario_modificacao     String     @db.VarChar(50)
  dt_criacao                 DateTime   @db.DateTime(0)
  dt_modificacao             DateTime   @db.DateTime(0)
  st_ativo                   Boolean    @default(dbgenerated("b'1'")) @db.Bit(1)
  TB_CENARIO                 TB_CENARIO @relation(fields: [id_cenario], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "TB_CENA_AMBIENTE_CENARIO")
  TB_CENA                    TB_CENA[]

  @@index([id_cenario], map: "TB_CENA_AMBIENTE_CENARIO_IDX")
}

model TB_AMBIENTE_SINONIMO {
  id                     BigInt   @id @default(autoincrement())
  id_ambiente            BigInt
  nm_ambiente            String   @db.VarChar(50)
  cd_usuario_criacao     String   @db.VarChar(50)
  cd_usuario_modificacao String   @db.VarChar(50)
  dt_criacao             DateTime @db.DateTime(0)
  dt_modificacao         DateTime @db.DateTime(0)
}

model TB_ATOR {
  id                        BigInt                      @id
  nm_ator                   String                      @db.VarChar(50)
  st_ativo                  Boolean                     @default(dbgenerated("b'1'")) @db.Bit(1)
  cd_usuario_criacao        String                      @db.VarChar(50)
  cd_usuario_modificacao    String                      @db.VarChar(50)
  dt_criacao                DateTime                    @db.DateTime(0)
  dt_modificacao            DateTime                    @db.DateTime(0)
  tp_genero                 Int?                        @db.TinyInt
  disponivel                Boolean?
  TB_INDISPONIBILIDADE_ATOR TB_INDISPONIBILIDADE_ATOR[]
  TB_PERSONAGEM             TB_PERSONAGEM[]
}

model TB_CAPITULO {
  id                     BigInt              @id @default(autoincrement())
  id_programa            BigInt
  id_numero              Int
  id_bloco               BigInt?
  id_st_capitulo         Int?                @db.TinyInt
  id_versao              String?             @db.Char(32)
  nm_versao              String?             @db.Char(5)
  nm_arquivo             String?             @db.VarChar(50)
  nm_episodio            String?             @db.VarChar(50)
  st_secreto             Boolean?            @db.Bit(1)
  st_ativo               Boolean             @default(dbgenerated("b'1'")) @db.Bit(1)
  dt_exibicao            DateTime?           @db.DateTime(0)
  dt_gravacao            DateTime?           @db.DateTime(0)
  dt_arquivo             DateTime            @db.DateTime(0)
  dt_criacao             DateTime            @db.DateTime(0)
  dt_modificacao         DateTime            @db.DateTime(0)
  cd_usuario_criacao     String              @db.VarChar(50)
  cd_usuario_modificacao String              @db.VarChar(50)
  id_tamanho_original    Decimal?            @db.Decimal(10, 2)
  TB_CAPITULO_BLOCO      TB_CAPITULO_BLOCO?  @relation(fields: [id_bloco], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "TB_CAPITULO_BLOCO")
  TB_STATUS_CAPITULO     TB_STATUS_CAPITULO? @relation(fields: [id_st_capitulo], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "TB_CAPITULO_STATUS_CAPITULO")
  TB_CENA                TB_CENA[]

  @@index([id_bloco], map: "TB_CAPITULO_BLOCO")
  @@index([id_st_capitulo], map: "TB_CAPITULO_STATUS_CAPITULO")
  @@index([id_numero], map: "TB_CAPITULO_id_numero_IDX")
  @@index([id_programa], map: "TB_CAPITULO_id_programa_IDX")
}

model TB_CAPITULO_BLOCO {
  id                     BigInt        @id @default(autoincrement())
  id_programa            BigInt
  id_tamanho             Int           @db.TinyInt
  id_numero              Int
  nm_bloco               String        @db.VarChar(50)
  cd_usuario_criacao     String        @db.VarChar(50)
  cd_usuario_modificacao String        @db.VarChar(50)
  dt_criacao             DateTime      @db.DateTime(0)
  dt_modificacao         DateTime      @db.DateTime(0)
  TB_CAPITULO            TB_CAPITULO[]

  @@unique([id_numero, id_programa], map: "UNIQUE_NUMERO_PROGRAMA")
  @@index([id_programa], map: "TB_CAPITULO_BLOCO_id_programa_IDX")
}

model TB_CATEGORIA_CENA {
  id           Int       @id @db.TinyInt
  nm_categoria String    @db.VarChar(50)
  TB_CENA      TB_CENA[]
}

model TB_CATEGORIA_OCORRENCIA {
  id            BigInt          @id @unique(map: "ID_UNIQUE")
  id_pai        BigInt?
  nm_categoria  String          @db.VarChar(50)
  st_ativo      Boolean         @default(dbgenerated("b'1'")) @db.Bit(1)
  st_elenco     Boolean         @default(dbgenerated("b'0'")) @db.Bit(1)
  TB_OCORRENCIA TB_OCORRENCIA[]

  @@index([id_pai], map: "CATEGORIA_PAI_IDX")
}

model TB_CENA {
  id                                                               BigInt                     @id @default(autoincrement())
  id_numero                                                        Int?                       @db.TinyInt
  id_pagina                                                        Int                        @default(0) @db.TinyInt
  id_cenario                                                       BigInt?
  id_tamanho                                                       Decimal                    @default(0.000) @db.Decimal(10, 3)
  id_ambiente                                                      BigInt?
  id_tp_periodo_cenico                                             Int?                       @db.TinyInt
  id_tp_hora_cenica                                                Int?                       @db.TinyInt
  id_tp_espaco_cena                                                Int?                       @db.TinyInt
  id_tp_categoria_cena                                             Int?                       @db.TinyInt
  id_tp_classificacao_cena                                         Int?                       @db.TinyInt
  id_st_cena                                                       Int                        @default(1) @db.TinyInt
  id_tp_acesso                                                     Int?                       @db.TinyInt
  id_numero_dias_antes_gravacao                                    Int?                       @db.TinyInt
  cd_letra                                                         String?                    @db.Char(2)
  st_secreta                                                       Boolean                    @default(dbgenerated("b'0'")) @db.Bit(1)
  st_gravar                                                        Boolean                    @default(dbgenerated("b'1'")) @db.Bit(1)
  st_sem_ambiente                                                  Boolean                    @default(dbgenerated("b'0'")) @db.Bit(1)
  st_ativo                                                         Boolean                    @default(dbgenerated("b'1'")) @db.Bit(1)
  ds_cenario_temporario                                            String?                    @db.VarChar(100)
  ds_ambiente_temporario                                           String?                    @db.VarChar(100)
  ds_resumo                                                        String?                    @db.VarChar(10000)
  cd_usuario_criacao                                               String                     @db.VarChar(50)
  cd_usuario_modificacao                                           String                     @db.VarChar(50)
  dt_expiracao_sigilo                                              DateTime?                  @db.DateTime(0)
  dt_criacao                                                       DateTime                   @db.DateTime(0)
  dt_modificacao                                                   DateTime                   @db.DateTime(0)
  id_capitulo                                                      BigInt
  ds_observacao                                                    String?                    @db.VarChar(280)
  TB_CATEGORIA_CENA                                                TB_CATEGORIA_CENA?         @relation(fields: [id_tp_categoria_cena], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_cena_categoria_cena")
  TB_TIPO_CENA                                                     TB_TIPO_CENA?              @relation(fields: [id_tp_classificacao_cena], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_cena_classificacao_cena")
  TB_TIPO_ESPACO_CENA                                              TB_TIPO_ESPACO_CENA?       @relation(fields: [id_tp_espaco_cena], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_cena_espaco_cena")
  TB_TIPO_PERIODO_CENICO                                           TB_TIPO_PERIODO_CENICO?    @relation(fields: [id_tp_periodo_cenico], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_cena_periodo_cenico")
  TB_AMBIENTE                                                      TB_AMBIENTE?               @relation(fields: [id_ambiente], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "tb_cena_ambiente")
  TB_CENARIO                                                       TB_CENARIO?                @relation(fields: [id_cenario], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "tb_cena_cenario")
  TB_CAPITULO                                                      TB_CAPITULO                @relation(fields: [id_capitulo], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "tb_cena_tb_capitulo")
  TB_CENA_FIGURANTE                                                TB_CENA_FIGURANTE[]
  TB_CENA_PERSONAGEM                                               TB_CENA_PERSONAGEM[]
  TB_CENA_SEQUENCIA_TB_CENA_SEQUENCIA_id_cenaToTB_CENA             TB_CENA_SEQUENCIA[]        @relation("TB_CENA_SEQUENCIA_id_cenaToTB_CENA")
  TB_CENA_SEQUENCIA_TB_CENA_SEQUENCIA_id_cena_relacionadaToTB_CENA TB_CENA_SEQUENCIA[]        @relation("TB_CENA_SEQUENCIA_id_cena_relacionadaToTB_CENA")
  TB_CENA_STATUS_HISTORICO                                         TB_CENA_STATUS_HISTORICO[]
  TB_CENA_TAG                                                      TB_CENA_TAG[]
  TB_CENA_TEXTO                                                    TB_CENA_TEXTO[]
  TB_DUBLE_PERSONAGEM_CENA                                         TB_DUBLE_PERSONAGEM_CENA[]
  TB_ROTEIRO_CENA                                                  TB_ROTEIRO_CENA[]

  @@index([cd_letra], map: "LETRA_IX")
  @@index([id_capitulo], map: "TB_CENA_TB_CAPITULO_id_fk")
  @@index([id_numero], map: "TB_CENA_id_numero_IDX")
  @@index([st_ativo], map: "TB_CENA_st_ativo_IDX")
  @@index([id_ambiente], map: "ambiente_ix")
  @@index([id_cenario], map: "cenario_ix")
  @@index([id_tp_categoria_cena], map: "fk_cena_categoria_cena_idx")
  @@index([id_tp_classificacao_cena], map: "fk_cena_classificacao_cena_idx")
  @@index([id_tp_espaco_cena], map: "fk_cena_espaco_cena_idx")
  @@index([id_tp_periodo_cenico], map: "fk_cena_periodo_cenico_idx")
}

model TB_CENARIO {
  id                     BigInt                @id @default(autoincrement())
  id_programa            BigInt
  nm_cenario             String                @db.VarChar(50)
  cd_usuario_criacao     String                @db.VarChar(50)
  cd_usuario_modificacao String                @db.VarChar(50)
  dt_criacao             DateTime              @db.DateTime(0)
  dt_modificacao         DateTime              @db.DateTime(0)
  st_ativo               Boolean               @default(dbgenerated("b'1'")) @db.Bit(1)
